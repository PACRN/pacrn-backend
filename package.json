{
  "name": "pacrn_backend",
  "version": "1.0.0",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev-run": "yarn build && yarn dev",
    "dev-local": "yarn build && yarn local",
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/main.ts",
    "build": "rimraf dist && ./node_modules/.bin/tsc -p tsconfig.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrations": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run -d src/utilities/orm.ts",
    "dev": "./node_modules/.bin/env-cmd -f .env node ./dist/main.js",
    "local": "./node_modules/.bin/env-cmd -f .env.local node ./dist/main.js",
    "create-db-dev": "./node_modules/.bin/env-cmd -f .env ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d src/utilities/data-source.ts -s no",
    "create-db-local": "./node_modules/.bin/env-cmd -f .env.local ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d src/utilities/data-source.ts -s no",
    "drop-db-dev": "./node_modules/.bin/env-cmd -f .env ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d src/utilities/data-source.ts",
    "drop-db-local": "./node_modules/.bin/env-cmd -f .env.local ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d src/utilities/data-source.ts"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.25.0",
    "@google-cloud/vision": "^3.1.2",
    "@sendgrid/mail": "^8.1.3",
    "@types/minio": "^7.0.18",
    "@types/swagger-stats": "^0.95.8",
    "aws-sdk": "^2.1348.0",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "envalid": "^7.3.1",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "html-to-text": "^8.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "minio": "",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "pacrn_backend": "file:",
    "pdf-lib": "^1.17.1",
    "pg": "^8.4.0",
    "pug": "^3.0.2",
    "puppeteer": "^23.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.4",
    "swagger-parser": "^10.0.3",
    "swagger-stats": "^0.99.5",
    "tesseract.js": "^4.0.3",
    "text-from-image": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.16",
    "typeorm-extension": "^2.6.2",
    "uuid": "^9.0.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^8.1.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "@types/pug": "^2.0.6",
    "@types/sharp": "^0.30.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "morgan": "^1.10.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.2"
  }
}
